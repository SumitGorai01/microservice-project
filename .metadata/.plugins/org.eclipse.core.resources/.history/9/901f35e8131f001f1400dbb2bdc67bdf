spring.application.name=RatingService

#change port
server.port=9094

# Database configuration
spring.data.mongodb.uri= mongodb://localhost:27017
spring.data.mongodb.database= microservices

#spring.config.import=optional:configserver:http://localhost:9097

#for eureka server
eureka.instance.prefer-ip-address= true
eureka.client.register-with-eureka = true
eureka.client.fetch-registry = true
eureka.client.service-url.defaultZone = http://localhost:9095/eureka